{
	"name": "@yeungkc/svelte-portal",
	"description": "A Portal library for Svelte. Supports SSR and Portal updates.",
	"version": "0.0.10",
	"author": {
		"name": "YeungKC",
		"email": "svelte-portal@YeungKC.com",
		"url": "https://YeungKC.com/"
	},
	"homepage": "https://github.com/YeungKC/svelte-portal/",
	"keywords": [
		"svelte",
		"sveltejs",
		"svelteKit",
		"portal",
		"SSR"
	],
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build-example": "vite build",
		"build": "svelte-kit sync && svelte-package",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-static": "^1.0.0-next.49",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"nanoid": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.2",
		"vite": "^4.4.9"
	},
	"type": "module"
}
